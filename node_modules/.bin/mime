#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/cursos/rest-api-express/node_modules/mime/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/cursos/rest-api-express/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/cursos/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/node_modules:/mnt/c/Users/elcos/OneDrive/node_modules:/mnt/c/Users/elcos/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/cursos/rest-api-express/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/cursos/rest-api-express/node_modules/mime/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/cursos/rest-api-express/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/cursos/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/node_modules:/mnt/c/Users/elcos/OneDrive/node_modules:/mnt/c/Users/elcos/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/elcos/OneDrive/Escritorio/PAGINAS/cursos/rest-api-express/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
